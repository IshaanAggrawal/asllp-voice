                        "url": {
                            "raw": "{{BASE_URL}}/api/agents/{{agent_id}}/",
                            "host": [
                                "{{BASE_URL}}"
                            ],
                            "path": [
                                "api",
                                "agents",
                                "{{agent_id}}",
                                ""
                            ]
                        }
                    }
                },
                {
                    "name": "Delete Agent",
                    "request": {
                        "method": "DELETE",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{BASE_URL}}/api/agents/{{agent_id}}/",
                            "host": [
                                "{{BASE_URL}}"
                            ],
                            "path": [
                                "api",
                                "agents",
                                "{{agent_id}}",
                                ""
                            ]
                        }
                    }
                },
                {
                    "name": "Start Session",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "pm.test(\"Session created\", function () {",
                                    "    pm.response.to.have.status(201);",
                                    "    var jsonData = pm.response.json();",
                                    "    pm.collectionVariables.set(\"session_id\", jsonData.id);",
                                    "});"
                                ],
                                "type": "text/javascript"
                            }
                        }
                    ],
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{BASE_URL}}/api/agents/{{agent_id}}/start_session/",
                            "host": [
                                "{{BASE_URL}}"
                            ],
                            "path": [
                                "api",
                                "agents",
                                "{{agent_id}}",
                                "start_session",
                                ""
                            ]
                        }
                    }
                }
            ]
        },
